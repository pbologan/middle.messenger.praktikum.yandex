{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "Messenger",
  "scripts": {
    "start": "rm -rf ./dist ./.parcel-cache && npm run build && npm run start.server",
    "start.dev": "rm -rf ./dist ./.parcel-cache && npm run lint && npm run stylelint && parcel src/index.html",
    "start.server": "node server.js",
    "build": "parcel build src/index.html --no-source-maps --no-cache --public-url ./",
    "parcel.watch": "parcel watch src/index.html --no-source-maps --no-cache --public-url ./",
    "lint": "eslint '**/*.{ts,tsx}' --fix",
    "stylelint": "npx stylelint '**/*.css' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pbologan/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "Pavel Bologan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pbologan/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/pbologan/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@types/node": "18.7.2",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "parcel": "2.8.3",
    "postcss-import": "14.1.0",
    "postcss-nested": "5.0.6",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "nanoid": "4.0.1"
  }
}
